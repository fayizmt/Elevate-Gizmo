<style>
  .logo-name{
    margin-left: -100px;
    color: red;
} 

.remove-btn {
  background-color: transparent;
  border: 1px solid transparent;
  color: black;
  padding: 5px 10px;
  transition: background-color 0.3s ease;
}

.remove-btn:hover {
  background-color: red;
  color: white;
}
/* Cart Table Styles */
.cart-section {
    padding: 50px 0;
}

.cart-table {
    width: 100%;
    margin-bottom: 20px;
    background-color: #fff;
    border-radius: 10px;
    box-shadow: 0 0 20px rgba(0, 0, 0, 0.1);
}

.cart-table th,
.cart-table td {
    padding: 15px;
    vertical-align: middle;
}

.cart-table th {
    background-color: #f5f5f5;
    font-weight: bold;
    text-transform: uppercase;
    color: #333;
}

.cart-table td.product-name p {
    font-weight: bold;
    color: #333;
}

.cart-table td.product-price p,
.cart-table td.product-total p {
    font-weight: bold;
    color: #333;
}

.cart-table .row-close a i {
    color: #ccc;
    font-size: 18px;
    transition: color 0.3s ease;
}

.cart-table .row-close a i:hover {
    color: #ff0000;
}

/* Quantity Input and Buttons */
.product_count {
    display: flex;
    align-items: center;
}

.quantity-number {
    width: 50px;
    text-align: center;
    margin: 0 10px;
    border: 1px solid #ccc;
    border-radius: 5px;
    padding: 5px;
}

.btn-num-product-down,
.btn-num-product-up {
    background-color: #007bff;
    color: #fff;
    padding: 5px;
    border: none;
    border-radius: 5px;
    cursor: pointer;
    transition: background-color 0.3s ease;
}

.btn-num-product-down:hover,
.btn-num-product-up:hover {
    background-color: #0056b3;
}

/* Cart Total Box */
.cart-inner-box {
    border: 1px solid #ccc;
    padding: 20px;
    margin-bottom: 20px;
    background-color: #f9f9f9;
    border-radius: 10px;
}

.ci-title h6 {
    margin-bottom: 10px;
    font-weight: bold;
    color: #333;
}

.ci-caption ul {
    list-style: none;
    padding: 0;
}

.ci-caption ul li {
    margin-bottom: 10px;
    font-size: 16px;
    color: #333;
}




</style>
