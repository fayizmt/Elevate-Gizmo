                <div class="banner_slide_content center-align">
                    <div class="container">
                        <div class="row">
                            <div class="col-xl-4 col-lg-5 col-md-6 col-sm-7 col-9">
                                <div class="slider-caption text-center">
                                    <h1 class="animation" data-animation="fadeInDown" data-animation-delay="0.2s">SUMMER
                                        SALE</h1>
                                    <p class="animation" data-animation="fadeInUp" data-animation-delay="0.6s">Get up to
                                        50% off your shopping items tomorrow!</p>
                                    <a href="/shop" class="btn btn-primary animation"
                                        data-animation="fadeInUp" data-animation-delay="1s">Shop Now</a>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <% } %>
        </div>
        <ol class="carousel-indicators">
            <% for (let i = 0; i < bannerList.length; i++) { %>
            <li data-target="#carouselExampleControls" data-slide-to="<%= i %>"
                class="<%= i === 0 ? 'active' : '' %>"></li>
            <% } %>
        </ol>
    </div>
</section>



